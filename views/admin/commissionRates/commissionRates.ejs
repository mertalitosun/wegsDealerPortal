<!DOCTYPE html>
<html lang="en">
<%- include('../../partials/head') %>
<body>
    <%- include('../../partials/nav') %>
    <div class="container mt-5 d-flex justify-content-center">
        <div class="col-lg-8 table-responsive">
            <table class="table table-bordered table-striped">
                <thead>
                    <tr>
                        <th>Komisyon Adı</th>
                        <th>Komisyon Oranı</th>
                    </tr>
                </thead>
                <tbody>
                    <% commissionRates.forEach(commissionRate => { %>
                        <tr>
                            <td>
                            <%= commissionRate.name %>
                            </td>
                            <td>
                                <%= commissionRate.rate %>
                                </td>
                            <td>
                                <a href="/admin/commissionRate/edit/<%= commissionRate.id %>"><i class="bi bi-pencil-square"></i></a>
                            </td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
    <%- include('../../partials/script') %>
</body>
</html>